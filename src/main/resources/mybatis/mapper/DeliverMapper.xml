<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-/mabatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.exe.cozy.mapper.DeliverMapper">

    <select id="maxNumDeliver" resultType="int">
        select nvl(max(deliverNum),0) from deliver
    </select>

    <insert id="insertDeliver" parameterType="DeliverDto">
        insert into deliver (deliverNum, customerEmail, deliverName,deliverAddr1,deliverAddr2,deliverAddrd,delivertel)
        values (#{deliverNum}, #{customerEmail}, #{deliverName}, #{deliverAddr1}, #{deliverAddr2}, #{deliverAddrd}, #{delivertel})
    </insert>


</mapper>